    "lint:noFix": "yarn run lint:src:noFix && yarn run lint:spec:noFix",
    "lint:src:noFix": "eslint --config .eslintrc-for-source.yaml --ignore-pattern '**/*.spec.ts' --ignore-pattern '**/*.spec.tsx' './src/**/*.{ts,tsx}'",
