    "lint:src:noFix:win": "eslint --config .eslintrc-for-source.yaml --ignore-pattern \"*.spec.*\" \"src\\**\\*.{ts,tsx}\"",
    "lint:spec:noFix:win": "eslint --config .eslintrc-for-spec.yaml \"src\\**\\*.spec.{ts,tsx}\"",
